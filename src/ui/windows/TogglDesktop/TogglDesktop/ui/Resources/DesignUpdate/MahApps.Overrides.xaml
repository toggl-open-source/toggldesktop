<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="AccentColorBrush" Color="{DynamicResource Toggl.AccentColor}" />
    <SolidColorBrush x:Key="AccentSelectedColorBrush" Color="{DynamicResource Toggl.PrimaryTextColor}" />
    <SolidColorBrush x:Key="TextBoxBorderBrush" Color="{DynamicResource Toggl.DisabledTextColor}" />
    <SolidColorBrush x:Key="TextBrush" Color="{DynamicResource Toggl.PrimaryTextColor}" />
</ResourceDictionary>